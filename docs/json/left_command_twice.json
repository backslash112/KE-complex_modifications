{
  "title": "CMDx2_2",
  "rules": [
    {
      "description": "Open Afred by pressing command twice",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "type": "variable_if",
              "name": "command-q",
              "value": 1
            }
          ],
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "q",
              "modifiers": ["left_shift", "left_control"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "command-q",
                "value": 1
              }
            },
            {
              "key_code": "left_command"
            }
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "command-q",
                  "value": 0
                }
              }
            ],
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "command-q",
                  "value": 0
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
